{
  "name": "@v-anything/directives",
  "type": "module",
  "version": "0.0.6",
  "description": "A Vue 3 custom directives library.",
  "author": {
    "name": "summergua",
    "email": "covey.shu@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://v-anything.github.io/v-anything",
  "repository": "@v-anything/directives",
  "keywords": [
    "vue"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./dev": "./src/index.ts"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist/*.d.ts",
    "dist/*.js",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w --watch.onEnd=\"node ./log.js\" -c rollup.config.js"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.0.0"
  }
}
